<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Registro</servlet-name>
        <servlet-class>Registro.Registro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Jugar</servlet-name>
        <servlet-class>JuegosNumeros.Jugar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MostrarDatos</servlet-name>
        <servlet-class>BaseDatos.MostrarDatos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registro</servlet-name>
        <url-pattern>/Registro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Jugar</servlet-name>
        <url-pattern>/Jugar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MostrarDatos</servlet-name>
        <url-pattern>/MostrarDatos</url-pattern>
    </servlet-mapping>
</web-app>
